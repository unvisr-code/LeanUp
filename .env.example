# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Resend Configuration
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=your_resend_api_key_here

# Admin email to receive lead notifications
ADMIN_EMAIL=admin@leanup.kr

# Email Domain Verification (optional)
# Set to 'true' only after verifying your domain in Resend Dashboard
# If not set or 'false', will use Resend's onboarding domain (onboarding@resend.dev)
# EMAIL_DOMAIN_VERIFIED=false

# Slack Configuration (optional)
# Get your Webhook URL from: https://api.slack.com/messaging/webhooks
# If not set, Slack notifications will be skipped
# SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
